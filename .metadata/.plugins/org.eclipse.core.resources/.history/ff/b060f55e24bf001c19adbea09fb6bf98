package variavel;

import java.util.Scanner;

/*11)	Escreva um programa em java para ler uma temperatura em graus Fahrenheit, calcular e 
 * escrever o valor correspondente em graus Celsius (baseado na fórmula abaixo): 
 	C 	 	F - 32  
 	---------- 	= 	-----------  
 	5 	 	9 	 
  Observação: Para testar se a sua resposta está correta saiba que  100oC  =  212F
 * */
public class Fixacao9 {

	public static void main(String[] args) {
		double c;
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite a temperatura em Graus Fº");
		double grausF = sc.nextDouble();
		
		c = (grausF - 32)/59;
		
		System.out.println("a temperatura é "+c);
		

	}

}
