package variavel;

public class Variavel {

		/*$ Tipos de dados Primitivos
		 * boolean
		 * caracter
		 * numero inteiro
		 * numero com ponto flutuante*/ 
	
	
	public static void main(String[] args) {

		int i = 5;
		
		System.out.println("O valor da Variável i é: "+ i);
		
		System.out.println("O valor da Variável i é: "+ i++);

	}

}
